<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="website_extended_custom_snippets" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="after">
            <div class="o_panel_header">
                Custom Snippets
            </div>
            <div class="o_panel_body">
                <t t-snippet="theme_alan.seminar_training"
                   t-thumbnail="/website/static/src/img/snippets_thumbs/s_mega_menu_multi_menus.svg"/>
                <t t-snippet="theme_alan.IhreAnsprechpartner"
                   t-thumbnail="/website/static/src/img/snippets_thumbs/s_mega_menu_multi_menus.svg"/>
                <t t-snippet="theme_alan.Seminare"
                   t-thumbnail="/website/static/src/img/snippets_thumbs/s_mega_menu_multi_menus.svg"/>
                <t t-snippet="theme_alan.ServicefürSie"
                   t-thumbnail="/website/static/src/img/snippets_thumbs/s_mega_menu_multi_menus.svg"/>
            </div>
        </xpath>
    </template>

    <template id="seminar_training" name="Seminar / Training">
        <section class="seminar_training pt32 pb16">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 text-black">
                        <h6 class="font-weight-bold text-black">
                            Seminar / Training 3CX Pro
                        </h6>
                        <h6 class="pb32 text-black">
                            <span class="font-weight-bold">
                                Installation und Übersicht 3CX
                            </span>
                            <span>
                                – Grundlagen VOIP – Übersicht über die 3CX Telefonanlage – Installation und
                                Ersteinrichtung – Gateways definieren – VOIP Provider – DID – Firewall Einstellungen
                                –
                                NAT und PAT? - Statische und dynamische IP Adressen
                            </span>
                        </h6>
                        <h6 class="pb32 text-black">
                            <span class="font-weight-bold">
                                Administrationder 3CX Server
                            </span>
                            <span>
                                – Richtlinien für die Telefonie erstellen (Outbound/Inbound) - Teilnehmer anlegen –
                                Rufnummern zuweisen – Richtlinien für die Weiterleitung – Sicherung und
                                Wiederherstellung -
                                Updates und Servicepacks installieren
                            </span>
                        </h6>
                        <h6 class="pb32 text-black">
                            <span class="font-weight-bold">
                                Monitoring und Troubleshooting
                            </span>
                            <span>
                                – Firewall Tester – Interne / externe IP Adresse – Logfiles – 3CX und die
                                Virtualisierung
                            </span>
                        </h6>
                        <h6 class="pb32 text-black">
                            <span class="font-weight-bold">
                                Erweiterungen
                            </span>
                            <span>
                                – Das Hotel Modul – Das Call Center Modul
                            </span>
                        </h6>
                        <h6 class="pb8">
                            <span class="font-weight-bold text-black">
                                Voraussetzungen
                            </span>
                        </h6>
                        <h6>
                            <a href="#" class="internal-link" style="color: #5ba0e2;">
                                Windowskenntnisse
                            </a>
                        </h6>
                    </div>
                </div>
            </div>
        </section>
    </template>

